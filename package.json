{
  "name": "template-api",
  "version": "0.0.1",
  "description": "Template for creating REST APIs.",
  "author": "BIGGER",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "nest build",
    "build:prod": "nest build && npm run sentry:sourcemaps",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:cd": "npm run migration:run && npm run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings=0",
    "test": "cross-env NODE_ENV=automated_tests jest",
    "test:watch": "cross-env NODE_ENV=automated_tests jest --watch",
    "test:cov": "cross-env NODE_ENV=automated_tests jest --coverage --no-cache --runInBand",
    "test:debug": "cross-env NODE_ENV=automated_tests node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "schema:drop": "cross-env NODE_ENV=development npm run typeorm -- schema:drop -d ./src/config/orm.config.ts",
    "schema:sync": "cross-env NODE_ENV=development npm run typeorm -- schema:sync -d ./src/config/orm.config.ts",
    "migration:create": "cross-var npm run typeorm -- migration:create ./data/migrations/$npm_config_name",
    "migration:generate": "cross-var npm run typeorm -- migration:generate -d ./src/config/orm.config.ts ./data/migrations/$npm_config_name",
    "migration:run": "npm run typeorm -- migration:run -d ./src/config/orm.config.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ./src/config/orm.config.ts",
    "fixtures": "cross-env NODE_ENV=development fixtures load ./data/fixture --sync --debug --dataSource=./src/config/orm.config.ts --require=ts-node/register --require=tsconfig-paths/register",
    "prepare": "husky install",
    "generate:crud": "yo generators/app/crud --force",
    "cognito:prepare": "cross-env ./data/scripts/create_user_pool.sh",
    "dev:prepare": "cross-env ./data/scripts/create_default_env.sh",
    "sentry:sourcemaps": "npx sentry-cli sourcemaps inject ./dist && npx sentry-cli sourcemaps upload ./dist"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@adminjs/express": "^6.1.0",
    "@adminjs/nestjs": "^6.1.0",
    "@adminjs/typeorm": "github:bigger-tech/adminjs-extension",
    "@aws-sdk/client-s3": "^3.609.0",
    "@aws-sdk/client-ses": "^3.606.0",
    "@aws-sdk/client-sns": "^3.606.0",
    "@aws-sdk/client-sqs": "^3.0.0",
    "@casl/ability": "^6.5.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/typeorm": "^10.0.1",
    "@sentry/cli": "^2.36.1",
    "@sentry/nestjs": "^8.30.0",
    "@sentry/profiling-node": "^8.30.0",
    "@ssut/nestjs-sqs": "^3.0.0",
    "@stellar/stellar-sdk": "^12.3.0",
    "adminjs": "^7.8.1",
    "amazon-cognito-identity-js": "^6.3.7",
    "aws-sdk": "^2.1651.0",
    "axios": "^1.7.7",
    "better-sqlite3": "^8.7.0",
    "change-case-all": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "highlight.js": "^11.10.0",
    "jsonapi-serializer": "^3.6.9",
    "jwks-rsa": "^3.1.0",
    "mysql2": "^3.6.5",
    "openai": "^4.55.3",
    "passport-jwt": "^4.0.1",
    "react-toastify": "^10.0.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "stripe": "^16.11.0",
    "styled-components": "^6.1.12",
    "toml": "^3.0.0",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional": "^0.5.0",
    "uuid4": "^2.0.3 "
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonapi-serializer": "^3.6.8",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/stripe": "^8.0.417",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "change-case": "^5.4.4",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm-fixtures-cli": "^4.0.0",
    "typescript": "^5.1.3",
    "yeoman-generator": "^7.3.2",
    "yo": "^5.0.0"
  }
}
